{
  "name": "@zeit-dev/ngrx-util",
  "version": "1.1.2",
  "description": "Utility classes, functions we commonly use across ngrx projects.",
  "repository": "https://github.com/cice/ngrx-util",
  "author": "Marian Theisen <marian@cice-online.net>",
  "license": "MIT",
  "private": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "devDependencies": {
    "@angular/compiler": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/core": "^8.1.2",
    "install-peers-cli": "^2.1.1",
    "ng-packagr": "^5.3.0",
    "tslib": "^1.9.0",
    "typescript": ">=3.4 <3.5"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "lib/public-api.ts",
      "umdModuleIds": {
        "@ngrx/store": "@ngrx/store",
        "@ngrx/effects": "@ngrx/effects"
      }
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish-dist": "yarn publish dist --access public",
    "install-peers": "install-peers"
  },
  "peerDependencies": {
    "@ngrx/effects": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "rxjs": "^6.5.2"
  }
}
